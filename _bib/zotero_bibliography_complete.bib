  shorttitle = {Water {{Supply}} and {{Ancient Society}} in the {{Lake Balkhash Basin}}},
  booktitle = {Socio-{{Environmental Dynamics}} along the {{Historical Silk Road}}},
  author = {Panyushkina, Irina P. and Macklin, Mark G. and Toonen, Willem H. J. and Meko, David M.},
