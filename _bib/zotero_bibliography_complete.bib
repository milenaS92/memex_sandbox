@article{HaentjensDekkerComputersupported2015,
  title = {Computer-Supported Collation of Modern Manuscripts: {{CollateX}} and the {{Beckett Digital Manuscript Project}}},
  shorttitle = {Computer-Supported Collation of Modern Manuscripts},
  author = {Haentjens Dekker, Ronald and van Hulle, Dirk and Middell, Gregor and Neyt, Vincent and van Zundert, Joris},
