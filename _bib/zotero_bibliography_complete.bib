@article{PenatiNotes2010,
  title = {Notes on the {{Birth}} of {{Russian Turkestan}}'s {{Fiscal System}}: {{A View}} from the {{Fergana Oblast}}'},
  shorttitle = {Notes on the {{Birth}} of {{Russian Turkestan}}'s {{Fiscal System}}},
  author = {Penati, Beatrice},
  date = {2010},
  journaltitle = {Journal of the Economic and Social History of the Orient},
  volume = {53},
  pages = {739--769},
  publisher = {{Brill}},
  issn = {0022-4995},
  abstract = {Just after the conquest of the Kokand khanate in 1876, the new Russian rulers carried out land surveying and land-tax assessment in the rich and densely populated Fergana province. These "organisation works" constituted the most consistent attempt ever to establish a cadastral system in Russian Turkestan. Differently from what would happen later, they incorporated tentative claims to bring under control the land rights of the Muslim rural population and to internalise the process of land-tax raising. In this context, Russian local officials tested alternative and partly contradictory policies, and their results served as a background experience for the Turkestan Statute of 1886. Au lendemain de la conquête du khanat de Kokand en 1876, le nouveau pouvoir russe fit réaliser une enquête topographique et fiscale dans la province riche et densément peuplée du Fergana. Ces « travaux d'organisation » constituèrent la tentative la plus cohérente d'établir un système cadastral dans le Turkestan russe. Contrairement à ce qu'il advint plus tard, ils prirent en compte les revendications d'administrateurs qui entendaient contrôler les droits fonciers de la population rurale musulmane et internaliser le processus de collecte des impôts. Dans ce contexte, les fonctionnaires russes locaux expérimentèrent différentes politiques, en partie contradictoires, dont les résultats furent à l'arrière-plan de la loi de 1886 sur le gouvernement du Turkestan.},
  eprint = {41305784},
  eprinttype = {jstor},
  file = {/Users/romanovienna/Dropbox/3.Reading/Zotero/ZoteroBooks/PenatiB/Penati 2010 - Notes on the Birth of Russian2.pdf},
  number = {5}
}

