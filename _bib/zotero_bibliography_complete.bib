@article{Ben-BassatUrban2018,
  title = {Urban {{Factionalism}} in {{Late Ottoman Gaza}}, c. 1875-1914: {{Local Politics}} and {{Spatial Divisions}}},
  shorttitle = {Urban {{Factionalism}} in {{Late Ottoman Gaza}}, c. 1875-1914},
  author = {Ben-Bassat, Yuval and Büssow, Johann},
  date = {2018-05-24},
  journaltitle = {Journal of the Economic and Social History of the Orient},
  volume = {61},
