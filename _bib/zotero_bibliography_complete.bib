@article{SanchezReturn2016,
  title = {The {{Return}} of the {{Guilds}}: {{A View}} from {{Early Modern Madrid}}},
  shorttitle = {The {{Return}} of the {{Guilds}}},
  author = {Sánchez, Nieto and Antolín, José and Llorente, Zofío and Carlos, Juan},
  date = {2016-12-01},
