}

@article{BhattacharyaBook2008,
  title = {The "{{Book}} of {{Will}}" of {{Petrus Woskan}} (1680-1751): {{Some Insights}} into the {{Global Commercial Network}} of the {{Armenians}} in the {{Indian Ocean}}},
