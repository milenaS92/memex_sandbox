@article{PapoutsakisQutbaddin2020,
  title = {Quṭbaddīn An-{{Nahrawālī}}’s (917-990/1511-1582) {{Treasure}} of {{Names}} and {{Other Ottoman}}-{{Era Arabic Treatises}} on the {{Art}} of the {{Muʿammā}}},
  author = {Papoutsakis, Nefeli},
  date = {2020-04-24},
  journaltitle = {Journal of Arabic and Islamic Studies},
  volume = {20},
  pages = {53--89},
  issn = {0806-198X},
  url = {https://journals.uio.no/JAIS/article/view/7905},
  urldate = {2020-04-29},
  abstract = {In Timurid times Persian littérateurs devised a new kind of logogriph (muʿammā) that differed con­siderably from the muʿammā as was known in the Arabic tradition. The most salient feature of the new, Persianate muʿammā, which is normally a couplet, is that it has two levels of meaning: an obvious or surface meaning (‘the poetic meaning’), and an encoded ‘riddle meaning’, which gives the clues to the solution of the riddle. Since the 16th century the new, Persianate muʿammā became very popular with Ottoman Turkish and Arabic littérateurs as well. In fact, to judge by the available evidence, it appears that the new muʿammā gradually became the most popular kind of literary riddle in Arabic. The present paper presents Quṭbaddīn an-Nahrawālī’s (917-990/1511-1582) Treasure of Names, the most influential Arabic treatise on the new muʿammā, which is modelled on earlier Persian treatises. It discusses the growth of the new muʿammā in Arabic, describes the rules applying to it as presented in an-Nahrawālī’s treatise, analyses several Arabic muʿammayāt cited in that treatise and concludes by mentioning some additional Ottoman-era Arabic treatises on the riddle that testify to the great popularity of the new muʿammā in Arabic until the late 19th century.},
  file = {/Users/romanovienna/Dropbox/3.Reading/Zotero/ZoteroBooks/PapoutsakisN/Papoutsakis 2020 - Quṭbaddīn an-Nahrawālī’s.pdf;/Users/romanovienna/Dropbox/3.Reading/Zotero/ZoteroApp/storage/7P3SILF5/7905.html},
  langid = {english}
}

