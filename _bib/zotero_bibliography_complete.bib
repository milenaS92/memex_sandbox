  title = {The {{Life}} of {{Texts}}: {{An Introduction}} to {{Literary Studies}}},
  shorttitle = {The {{Life}} of {{Texts}}},
  author = {Wurth, Kiene Brillenburg and Rigney, Ann},
  date = {2019-08-28},
