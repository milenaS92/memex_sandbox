@article{Green-MercadoMorisco2018,
  title = {Morisco {{Prophecies}} at the {{French Court}} (1602-1607)},
  author = {Green-Mercado, Mayte},
  date = {2018-03-14},
  journaltitle = {Journal of the Economic and Social History of the Orient},
  volume = {61},
  pages = {91--123},
  publisher = {{Brill}},
  issn = {1568-5209, 0022-4995},
  doi = {10.1163/15685209-12341444},
  abstract = {{$<$}section class="abstract"{$><$}h2 class="abstractTitle text-title my-1" id="d2580e2"{$>$}Abstract{$<$}/h2{$><$}p{$>$}This article presents a case study of a rebellion conspiracy organized by a group of Moriscos—Spanish Muslims forcibly converted to Catholicism—in the early seventeenth century. In order to carry out their plans, these Moriscos sought assistance from the French king Henry \textsc{iv} (r. 1589-1610). Analyzing a Morisco letter remitted to Henry \textsc{iv} and multiple archival sources, this article argues that prophecy served as a diplomatic language through which Moriscos communicated with the most powerful Mediterranean rulers of their time. A ‘connected histories’ approach to the study of Morisco political activity underscores the ubiquity of prophecies and apocalyptic expectations in the social life and political culture of the early modern Mediterranean. As a language of diplomacy, apocalyptic discourse allowed for minor actors such as the Moriscos to engage in politics in a language that was deemed mutually intelligible, and thus capable of transcending confessional boundaries.{$<$}/p{$><$}/section{$>$}},
  file = {/Users/romanovienna/Dropbox/3.Reading/Zotero/ZoteroBooks/Green-MercadoM/Green-Mercado 2018 - Morisco Prophecies at the.pdf;/Users/romanovienna/Dropbox/3.Reading/Zotero/ZoteroApp/storage/TFTVF922/article-p91_91.html},
  langid = {english},
  number = {1-2}
}

@article{Green-MercadoSpeaking2018,
  title = {Speaking the {{End Times}}: {{Early Modern Politics}} and {{Religion}} from {{Iberia}} to {{Central Asia}}},
