  file = {/Users/romanovienna/Dropbox/3.Reading/Zotero/ZoteroBooks/KorkiakangasT/Korkiakangas 2018 - Spelling variation in.pdf;/Users/romanovienna/Dropbox/3.Reading/Zotero/ZoteroApp/storage/2VHX2IS6/4668751.html},
