@article{EychenneProduction2013,
  title = {La Production Agricole de {{Damas}} et de La {{Ghū̄ta}} Au {{XIV}} e Siècle {{Diversité}}, Taxation et Prix Des Cultures Maraîchères d'après al-{{Jazarī}} (m. 739/1338)},
