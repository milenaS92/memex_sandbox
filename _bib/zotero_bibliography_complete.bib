}

@article{KhalekNotes2015,
  title = {Some {{Notes}} on the {{Representation}} of {{Non}}-{{Muslim Officials}} in al-{{Ǧahšiyārī}}'s (d. 331/942) {{Kitāb}} al-{{Wuzarāʾ}} Wa-l-Kuttāb},
  author = {Khalek, Nancy},
  date = {2015},
  journaltitle = {Arabica},
  volume = {62},
  pages = {503--520},
  issn = {0570-5398},
  abstract = {Rather than a handbook or chronological history of secretaries and viziers, or a straightforward guide to administrative terminology, the Kitāb al-Wuzarāʾ wa-l-kuttāb by Muḥammad b. al-ʿAbdūs al-Ǧahšiyārī (d. 331/942) is a collection of ʿAbbāsid-era narratives on the history of early Islamic administration. Related to other works in the genre adab al-kātib, the Kitāb al-Wuzarāʾ wa-l-kuttāb provides insight into one compiler's literary techniques as well as his social/cultural agenda. The portion of al-Ǧahšiyārī's work analyzed here focuses on key events in the Umayyad period and illustrates his apologetic stance on the non-Muslim foundations of ʿAbbāsid administrative structures especially well. Using short anecdotes that contain pithy, repetitive tropes of non-Muslim administrative officials' arrogance or incompetence, al-Ǧahšiyārī polemically mythologized a foundations narrative for early Islamic administrative history. Several studies of another portion of this text, on the ʿAbbāsid caliphs and the Barmakids, have determined the literary/historical value of al-Ǧahšiyārī's work. In these brief comments, four short anecdotes from a relatively understudied portion of the Kitāb al-Wuzarāʾ wa-l-kuttāb are assessed in order to demonstrate al-Ǧahšiyārī's mode of representation for the earliest stages of Islamic administration. Plutôt qu'un manuel ou une histoire chronologique des bureaucrates et des vizirs ou un simple guide d'initiation à la terminologie administrative, le Kitāb al-Wuzarāʾ wa-l-kuttāb de Muhammad b. al-ʿAbdūs al-Ǧahšiyārī (d. 331/942) est une collection de récits de l'époque abbasside sur la genèse de l'administration islamique. Lié à d'autres œuvres du genre adab al-kātib, le Kitāb al-Wuzarāʾ wa-l-kuttāb met en lumière certaines techniques littéraires du compilateur ainsi que ses préoccupations socio-culturelles. L'extrait de l'œuvre d'al-Ǧahšiyārī que nous analysons dans cet article met l'accent sur les événements marquants de la période omeyyade et illustre particulièrement bien sa position apologétique sur les fondations non musulmanes des structures administratives abbasides. En utilisant de courtes anecdotes qui contiennent des tropes répétitives et concises de l'arrogance ou de l'incompétence des responsables administratifs non-musulmans, al-Ǧahšiyārī mythologise de façon polémique un récit des fondations du début de l'histoire administrative islamique. Plusieurs études d'un autre extrait de ce même texte, centrées sur les califes abbassides et les Barmécides, ont déterminé la valeur littéraire et historique de l'œuvre d'al-Ǧahšiyārī. Dans les brefs commentaires qui suivent, quatre courtes anecdotes d'une portion relativement peu étudiée du Kitāb al-Wuzarāʾ wa-l-kuttāb sont évaluées afin de démontrer le mode de représentation des premières étapes de l'administration islamique par al-Ǧahšiyārī.},
  eprint = {24811614},
  eprinttype = {jstor},
  file = {/Users/romanovienna/Dropbox/3.Reading/Zotero/ZoteroBooks/KhalekN/Khalek 2015 - Some Notes on the.pdf},
  number = {4}
}

