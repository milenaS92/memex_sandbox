@article{Karimi-NiaContemporary2012,
  title = {Contemporary {{Qur}}’anic {{Studies}} in {{Iran}} and Its {{Relationship}} with {{Qur}}’anic {{Studies}} in the {{West}} / الدراسات القرآنية المعاصرة في إيران والدراسات القرآنية في اﻟﻐﺮﺏ},
  author = {Karimi-Nia, Morteza and كريمي نيا, مرتضى},
  date = {2012},
  journaltitle = {Journal of Qur'anic Studies},
  volume = {14},
  pages = {45--72},
  publisher = {{Edinburgh University Press}},
  issn = {1465-3591},
  abstract = {The status of tafsīr and Qur’anic studies in the Islamic Republic of Iran has changed significantly during recent decades. The essay provides an overview of the state of Qur’anic studies in Iran today, aiming to examine the extent of the impact of studies by Western scholars on Iranian academic circles during the last three decades and the relationship between them. As in most Islamic countries, the major bulk of academic activity in Iran in this field used to be undertaken by the traditional ‘ulamā’; however, since the beginning of the twentieth century and the establishment of universities and other academic institutions in the Islamic world, there has been increasing diversity and development. After the Islamic Revolution, many gradual changes in the structure and approach of centres of religious learning and universities have occurred. Contemporary advancements in modern sciences and communications technologies have gradually brought the institutions engaged in the study of human sciences to confront the new context. As a result, the traditional Shī‘ī centres of learning, which until 50 years ago devoted themselves exclusively to the study of Islamic law and jurisprudence, today pay attention to the teaching of foreign languages, Qur’anic sciences and exegesis, including Western studies about the Qur’an, to a certain extent, and recognise the importance of almost all of the human sciences of the West. تغيرت مكانة التفسير والدراسات القرآنية في جمهورية ﺇيران الإسلامية تغيرﺍﹰ كبيرﺍﹰ في العقود الأخيرة . ﺗﻘﺪﹼم هذه المقالة نبذة عن الوضع الحالي للدراسات القرآنية في ﺇيران، وﺗﻘﻴﹼﻢ مدى ﺗﺄﺛﻴﺮ الدراسات التي ينتجها باحثون غربيون على الأكاديميين الإيرانيين خلال العقود الثلاثة الأخيرة، والعلاقات بين الطرفين . ففي ﺇيران، كما الحال في معظم الدول الإسلامية الأخرى، كان معظم ﺍﻟﻌﻤﻞ الأكاديمي في هذا المجال حكرﺍﹰ على العلماء التقليديين . لكن منذ بداية القرن العشرين ومع إنشاء جامعات ومعاهد أكاديمية أخرى في العالم الإسلامي، ﺇزداد التنوع والتطور في هذا المجال . وبعد الثورة الإسلامية أخذ المنهج الأكاديمي المتبع في مراكز التعليم الديني والجامعات في التحول التدريجي . إذ رغمت التطورات المعاصرة في العلوم الحديثة وفي تقنيات الإتصال المعاهدﹶ المهتمة بالعلوم الإنسانية على أخذ السياق المعامر بعين الإعتبار . ولذا أخذت مراكز التعليم الشيعية التقليدية، التي ظلت إلى ما قبل خمسين سنة لا ﺗﺪﺭﹼﺱ إلا الفقه، ﺗﻌﺘﻨﻲ اليوم بتدريس اللغات الأجنبية والعلوم القرآنية والتفسير، بما فيها الدراسات الغربية عن القرآن، وصار تعترف بقيمة معظم العلوم الإنسانية في الغرب .},
  eprint = {41719815},
  eprinttype = {jstor},
  file = {/Users/romanovienna/Dropbox/3.Reading/Zotero/ZoteroBooks/Karimi-NiaM/Karimi-Nia 2012 - Contemporary Qur’anic Studies.pdf},
  number = {1}
}

@article{Karimi-NiaHistoriography2013,
  title = {The {{Historiography}} of the {{Qur}}'an in the {{Muslim World}}: {{The Influence}} of {{Theodor Nöldeke}} / تأريخ القرآن في العالم الاسلامي قبل ثيودور نولدكه وبعده},
  shorttitle = {The {{Historiography}} of the {{Qur}}'an in the {{Muslim World}}},
  author = {Karimi-Nia, Morteza and نيا, مرتضى کريمي ـ},
  date = {2013},
  journaltitle = {Journal of Qur'anic Studies},
  volume = {15},
  pages = {46--68},
  publisher = {{[Edinburgh University Press, Centre for Islamic Studies at SOAS]}},
  issn = {1465-3591},
  abstract = {Almost 75 years after the initial publication of Theodor Nöldeke's Geschichte des Qorâns, Abū ʿAbd Allāh al-Zanjānī (1892–1941), an Iranian religious scholar, made a brief reference in the final chapter of his Tārīkh al-Qurʾān to an edition of Nöldeke's work revised by Schwally, Bergsträsser and Pretzl. He described Nöldeke as a great scholar and his Geschichte des Qorâns as a valuable contribution. Al-Zanjānī's work also included an outline of Nöldeke's discussion on the order of the revelation of the suras. It is probably safe to say that al-Zanjānī's book is the first independent work on the history of the Qur'an by a Muslim scholar. This fact leads one to wonder about the apparent lack of previous interest in the historiography of the Qur'an among Islamic scholarship. The present paper seeks to address this question and offer an overview of historiographical scholarship on the Qur'an contributed by Muslim writers both before and following Nöldeke بعد خمسة وسبعين سنة تقريبا من ظهور كتاب نولدكه بعنوان (تاريخ القرآن) أشار العالم الايراني أبو عبد اللّه الزنجاني (1892—1941) إشارة سريعة في الفصل الأخير من كتابه إلى نسخة من كتاب نولدكه بتنقيح شوالي وبرجستراسر وبريتزل. وقال الزنجاني إن نولدكه باحث عظيم وإن كتابه إسهام قيّم. واشتمل كتاب الزنجاني على ملخص لما ذكره نولدكه عن ترتيب وحي السور في القرآن. ولعله من غير المجازفة أن نقول إن كتاب الزنجاني أول عمل مستقل لباحث مسلم عن تاريخ القرآن، وهذا يدفعنا إلى التساؤل عن القصور الواضح من قبل في الاهتمام بتاريخ القرآن اهتماما كافيا في الكتابات الاسلامية. ويهدف هذا المقال إلى النظر في هذه المسألة كما يقدم عرضا عاما لكتابات الباحثين المسلمين في تاريخ القرآن قبل نولدكه وبعده.},
  eprint = {24280406},
  eprinttype = {jstor},
  file = {/Users/romanovienna/Dropbox/3.Reading/Zotero/ZoteroBooks/Karimi-NiaM/Karimi-Nia 2013 - The Historiography of the.pdf},
  number = {1}
}

