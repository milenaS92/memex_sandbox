@article{FitzmauriceLinguistic2017,
  title = {Linguistic {{DNA}}: {{Investigating Conceptual Change}} in {{Early Modern English Discourse}}},
  shorttitle = {Linguistic {{DNA}}},
  author = {Fitzmaurice, Susan and Robinson, Justyna A. and Alexander, Marc and Hine, Iona C. and Mehl, Seth and Dallachy, Fraser},
