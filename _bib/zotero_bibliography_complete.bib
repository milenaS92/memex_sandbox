@article{SanftLaw2010,
  title = {Law and {{Communication}} in {{Qin}} and {{Western Han China}}},
  author = {Sanft, Charles},
  date = {2010},
  journaltitle = {Journal of the Economic and Social History of the Orient},
  volume = {53},
  pages = {679--711},
  publisher = {{Brill}},
  issn = {0022-4995},
  abstract = {This article gathers and examines information about the legal practices of Qin and Western Han China, and demonstrates that these served communicative functions. Law in early imperial China not only penalized antisocial behavior, it was also a medium for communication between the central government, the common population, and officials. The information so transmitted comprised institutional and personal information, including that which facilitated the function of reputation on a national scale. There is also important evidence that people at the time recognized the communicative possibilities of legal practice, as reflected in cases where they manipulated penal communication for individual benefit. Cet article rassemble et examine des informations sur la jurisprudence des Qin ainsi que de la période des Han occidentaux prétendant quelles serviraient à des fonctions communicatives. Le droit au début des époques impériales servait donc ainsi, non seulement à pénaliser les comportements antisociaux-allant à l'encontre de la société-, mais également en tant que moyen de communications divers allant dans des dicrection différentes. Il est de même prouvé que, à cette époque, le gens reconnaissaient qu'il en était ainsi, particulièrement dans les cas où les manipulations ou bien les tentatives de manipulations des communiqués pénaux servaient au profit personel.},
  eprint = {41305782},
  eprinttype = {jstor},
  file = {/Users/romanovienna/Dropbox/3.Reading/Zotero/ZoteroBooks/SanftC/Sanft 2010 - Law and Communication in Qin.pdf},
  number = {5}
}

