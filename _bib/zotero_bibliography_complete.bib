@article{AstrakhantsevATR4S2017,
  title = {{{ATR4S}}: Toolkit with State-of-the-Art Automatic Terms Recognition Methods in {{Scala}}},
  shorttitle = {{{ATR4S}}},
  author = {Astrakhantsev, Nikita},
