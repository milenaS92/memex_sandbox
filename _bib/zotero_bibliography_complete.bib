@article{OpwisRole2011,
  title = {The {{Role}} of the {{Biographer}} in {{Constructing Identity}} and {{Doctrine}}: {{Al}}-{{ʿAbbādī}} and His "{{Kitāb Ṭabaqāt}} al-Fuqahāʾ al-Shāfiʿiyya"},
